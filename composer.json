{
	"name": "kronos/csp",
	"type": "library",
	"description": "A Content-Security-Policy header builder",
	"homepage": "https://github.com/kronostechnologies/kronos-csp",
	"license": "MIT",
	"keywords": [
		"kronos",
		"csp",
		"content-security-policy"
	],
	"autoload": {
		"psr-4": {
			"Kronos\\ContentSecurityPolicy\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kronos\\Tests\\ContentSecurityPolicy\\": "tests/"
		}
	},
	"require": {
		"php": "^7.4 || ^8.1",
		"cyclonedx/cyclonedx-php-composer": "3.9.2"
	},
	"require-dev": {
		"phpunit/phpunit": "9.5.20",
		"dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
		"phpcompatibility/php-compatibility": "9.3.5",
		"vimeo/psalm": "4.23.0"
	},
	"config": {
    "platform": {
      "php": "7.4"
    },
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"cyclonedx/cyclonedx-php-composer": true
		}
	}
}

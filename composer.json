{
	"name": "kronos/csp",
	"type": "library",
	"description": "A Content-Security-Policy header builder",
	"homepage": "https://github.com/kronostechnologies/kronos-csp",
	"license": "MIT",
	"keywords": [
		"kronos",
		"csp",
		"content-security-policy"
	],
	"autoload": {
		"psr-4": {
			"Kronos\\ContentSecurityPolicy\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kronos\\Tests\\ContentSecurityPolicy\\": "tests/"
		}
	},
	"require": {
		"php": ">=5.6",
		"cyclonedx/cyclonedx-php-composer": "3.9.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"phpcompatibility/php-compatibility": "^9.3",
		"vimeo/psalm": "4.22.0"
	},
	"config": {
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"cyclonedx/cyclonedx-php-composer": true
		}
	}
}
